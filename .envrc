#!/usr/bin/env bash
# direnv: load env vars automatically on cd

# Load env files if present
dotenv_if_exists .env.local
dotenv_if_exists .env.remote

# Add local node binaries to PATH
export PATH=$PWD/node_modules/.bin:$PATH

# Optionally pin Python via pyenv/.python-version; uv will also respect UV_PYTHON if set
# Uncomment to force a specific interpreter path
# export UV_PYTHON=python3.11

echo "[direnv] Environment loaded for adult_matching"

